{
    "manifest_version": 3,
    "name": "Snipe Explorer",
    "version": "0.0.1",
    "description": "Eagle-eye exploration for SRA experiments",
    "content_scripts": [
        {
            "matches": [
                "https://ncbi.nlm.nih.gov/sra/*",
                "https://ncbi.nlm.nih.gov/biosample/*",
                "https://ncbi.nlm.nih.gov/bioproject/*"
            ],
            "js": [
                "contentScript.js"
            ],
            "css": [
                "styles.css"
            ],
            "run_at": "document_end"
        }
    ]
}